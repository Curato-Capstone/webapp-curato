{
  "name": "webapp-curato",
  "version": "0.0.1",
  "description": "Web app for Curato",
  "scripts": {
    "start": "NODE_ENV=development node server.babel.js",
    "windows": "node server.babel.js",
    "serve": "NODE_ENV=production node server.babel.js",
    "nodemon": "NODE_ENV=development nodemon server.babel.js",
    "test": "mocha --compilers js:babel-core/register --require ./test/setup.js 'test/**/*.@(js|jsx)'",
    "lint": "eslint client/ server.js --color; exit 0",
    "flow": "flow --color=\"always\"; exit 0",
    "test:watch": "npm run test -- --watch --watch-extensions jsx",
    "lint:watch": "watch --color npm run lint",
    "flow:watch": "watch --color npm run flow"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Curato-Capstone/webapp-curato.git"
  },
  "author": "Curato",
  "license": "ISC",
  "homepage": "https://github.com/Curato-Capstone/webapp-curato#readme",
  "dependencies": {
    "autobind-decorator": "^1.3.3",
    "bluebird": "^3.1.1",
    "immutable": "^3.7.6",
    "koa": "^2.0.0",
    "koa-bodyparser": "^3.0.0",
    "koa-convert": "^1.2.0",
    "koa-router": "^7.0.1",
    "koa-send": "^3.2.0",
    "koa-static": "^2.0.0",
    "lodash": "^4.10.0",
    "material-ui": "^0.15.0-alpha.2",
    "radium": "^0.17.1",
    "react": "^15.0.0",
    "react-dom": "^15.0.0",
